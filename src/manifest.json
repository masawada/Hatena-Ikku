{
  "name": "Hatena Ikku",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "content_scripts": [{
    "matches": [
      "http://*.hatenablog.com/*",
      "http://*.hatenablog.jp/*",
      "http://*.hateblo.jp/*",
      "http://*.hatenadiary.com/*",
