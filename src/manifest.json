    "dict/tid_map.dat.gz",
    "dict/tid_pos.dat.gz",
    "dict/unk.dat.gz",
    "dict/unk_char.dat.gz",
    "dict/unk_compat.dat.gz",
    "dict/unk_invoke.dat.gz",
    "dict/unk_map.dat.gz",
    "dict/unk_pos.dat.gz",
    "lib/highlight.js"
  ]
